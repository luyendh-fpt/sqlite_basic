<Page
    x:Class="SQLiteAgain.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SQLiteAgain"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock HorizontalAlignment="Left" Margin="109,91,0,0" Text="Name" TextWrapping="Wrap" VerticalAlignment="Top" />
            <TextBox x:Name="StudentName" HorizontalAlignment="Left" Margin="173,84,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock HorizontalAlignment="Left" Margin="109,91,0,0" Text="Age" TextWrapping="Wrap" VerticalAlignment="Top" />
            <TextBox x:Name="StudentAge" HorizontalAlignment="Left" Margin="173,84,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top"/>
        </StackPanel>
        <Button Click="ButtonBase_OnClick">Save Linh</Button>
        <Button Click="ButtonBase_OnClick2">Go to List</Button>
    </StackPanel>
</Page>
